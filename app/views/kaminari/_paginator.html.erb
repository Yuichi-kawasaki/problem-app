<%= paginator.render do -%>
  <ul class="pagination">
    <%= first_page_tag unless current_page.first? %>
    <%= prev_page_tag unless current_page.first? %>
    <% each_page do |page| -%>
      <% if page.left_outer? || page.right_outer? || page.inside_window? -%>
        <%= page_tag page %>
      <% elsif !page.was_truncated? -%>
        <%= gap_tag %>
      <% end -%>
    <% end -%>
    <li class="pr-2"><%= next_page_tag unless current_page.last? %></li>
    <li class="pr-2"><%= last_page_tag unless current_page.last? %></li>
  </ul>
<% end -%>
